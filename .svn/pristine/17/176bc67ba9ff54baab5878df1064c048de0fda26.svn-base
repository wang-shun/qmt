            <!--<outputDirectory>lib</outputDirectory>-->
            <!--<excludes>-->
                <!--<exclude>com.lesports.qmt:qmt-sbd-import</exclude>-->
            <!--</excludes>-->
        <!--</dependencySet>-->
    </dependencySets>
</assembly>