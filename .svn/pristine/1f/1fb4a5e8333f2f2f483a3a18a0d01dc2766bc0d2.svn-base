package com.lesports.qmt.web.api.core.vo;

import java.util.List;

/**
 * Created by gengchengliang on 2016/5/12.
 */
public class MixedVo {

	//焦点图
	private List<RecommendVo> focus;
	//节目
	private List<HallEpisodeVo> episodes;
	//是否有赛程
	private Integer isMatch = 0;
	//是否有榜单
	private Integer isTopList = 0;
	//tag对应的赛事id
	private Long cid;

	public List<RecommendVo> getFocus() {
		return focus;
	}

	public void setFocus(List<RecommendVo> focus) {
		this.focus = focus;
	}

	public List<HallEpisodeVo> getEpisodes() {
		return episodes;
	}

	public void setEpisodes(List<HallEpisodeVo> episodes) {
		this.episodes = episodes;
	}

	public Integer getIsMatch() {
		return isMatch;
	}

	public void setIsMatch(Integer isMatch) {
		this.isMatch = isMatch;
	}

	public Integer getIsTopList() {
		return isTopList;
	}

	public void setIsTopList(Integer isTopList) {
		this.isTopList = isTopList;
	}

	public Long getCid() {
		return cid;
	}

	public void setCid(Long cid) {
		this.cid = cid;
	}

	@Override
	public String toString() {
		final StringBuffer sb = new StringBuffer("MixedVo{");
		sb.append("focus=").append(focus);
		sb.append(", episodes=").append(episodes);
		sb.append(", isMatch=").append(isMatch);
		sb.append(", isTopList=").append(isTopList);
		sb.append(", cid=").append(cid);
		sb.append('}');
		return sb.toString();
	}
}
