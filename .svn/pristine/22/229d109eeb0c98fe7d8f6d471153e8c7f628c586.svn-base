package com.lesports.qmt.web.model;


import com.lesports.qmt.sbd.api.dto.TTopListItem;

/**
 * Created by lufei1 on 2015/10/10.
 */
public class TeamTopListVo {
    //队伍id
    private Long tid;
    //队伍名称
    private String name;
    //联盟
    private String conference;
    //分区
    private String region;
    //联盟技术统计
    private TTopListItem conferenceItem;
    //分区技术统计
    private TTopListItem regionItem;

    public Long getTid() {
        return tid;
    }

    public void setTid(Long tid) {
        this.tid = tid;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public TTopListItem getConferenceItem() {
        return conferenceItem;
    }

    public void setConferenceItem(TTopListItem conferenceItem) {
        this.conferenceItem = conferenceItem;
    }

    public TTopListItem getRegionItem() {
        return regionItem;
    }

    public void setRegionItem(TTopListItem regionItem) {
        this.regionItem = regionItem;
    }

    public String getConference() {
        return conference;
    }

    public void setConference(String conference) {
        this.conference = conference;
    }

    public String getRegion() {
        return region;
    }

    public void setRegion(String region) {
        this.region = region;
    }

    @Override
    public String toString() {
        return "TeamTopListVo{" +
                "tid=" + tid +
                ", name='" + name + '\'' +
                ", conference='" + conference + '\'' +
                ", region='" + region + '\'' +
                ", conferenceItem=" + conferenceItem +
                ", regionItem=" + regionItem +
                '}';
    }
}
