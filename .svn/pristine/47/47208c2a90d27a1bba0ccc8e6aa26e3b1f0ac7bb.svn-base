public enum BusinessType {
    DATA_SYNC(1), //数据同步
    SEARCH_INDEX(2), //索引
    CBASE_DELETE(4),
    CMS_CBASE_DELETE(5),//删除cms页面cbase缓存
    /*业务分离&异步(6)*/;
    private int value;

    BusinessType(int value) {
        this.setValue(value);
    }

    public int getValue() {
        return value;
    }

    public void setValue(int value) {
        this.value = value;
    }
}
