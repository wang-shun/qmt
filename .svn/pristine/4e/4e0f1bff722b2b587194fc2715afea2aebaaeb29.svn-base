package com.lesports.qmt.sbc.vo;

import com.lesports.qmt.mvc.QmtVo;
import org.apache.commons.lang3.builder.ToStringBuilder;
import org.apache.commons.lang3.builder.ToStringStyle;

/**
 * 相关标签, 可包含赛事 赛程 球队 球员 自制专辑 自制节目 其他节目 话题
 * Created by denghui on 2016/11/26.
 */
public class RelatedTagVo implements QmtVo {
    private static final long serialVersionUID = 999887845422019487L;

    private Long id;
    private String name;

    public Long getId() {
        return id;
    }

    public void setId(Long id) {
        this.id = id;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    @Override
    public String toString() {
        return ToStringBuilder.reflectionToString(this, ToStringStyle.SHORT_PREFIX_STYLE);
    }
}
