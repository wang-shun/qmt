package com.lesports.qmt.web.api.core.vo;


import java.io.Serializable;
import java.util.List;

/**
 * Created by ruiyuansheng on 2016/8/24.
 */
public class MenuVo implements Serializable {

    private static final long serialVersionUID = 1834282224654730748L;
    private long id;
    private String name;
    private List<MenuItemVo> items;

    public static class MenuItemVo implements Serializable {

        private static final long serialVersionUID = 4921124173499580198L;

        private long index;
        private int resourceType;
        private long resourceId;
        private int level;
        private List<MenuItemVo> subItems;
        private String name;
        private int isNew;
        private int isHot;
        private int order;
        private String imageUrl;
        private int isSuggest;
        private int isMatch;
        private int isTopList;
        private String blockId;
        private int isRecommend;
        private long cid;
        private String h5Url;
        private int recommendOrder;
        private String campCategoryId;
        private String campId; // optional
        private int menuRecommendType;
        private int forceRecommendOrder;
        private int scheduleType;
        private int isAuthority;//针对某个渠道是否有权限
        private int isTeamData;//是否展示球队数据
        private int isPlayerData;//是否展示球员数据

        public int getIsAuthority() {
            return isAuthority;
        }

        public void setIsAuthority(int isAuthority) {
            this.isAuthority = isAuthority;
        }

        public long getIndex() {
            return index;
        }

        public void setIndex(long index) {
            this.index = index;
        }

        public int getResourceType() {
            return resourceType;
        }

        public void setResourceType(int resourceType) {
            this.resourceType = resourceType;
        }

        public long getResourceId() {
            return resourceId;
        }

        public void setResourceId(long resourceId) {
            this.resourceId = resourceId;
        }

        public int getLevel() {
            return level;
        }

        public void setLevel(int level) {
            this.level = level;
        }

        public List<MenuItemVo> getSubItems() {
            return subItems;
        }

        public void setSubItems(List<MenuItemVo> subItems) {
            this.subItems = subItems;
        }

        public String getName() {
            return name;
        }

        public void setName(String name) {
            this.name = name;
        }

        public int getIsNew() {
            return isNew;
        }

        public void setIsNew(int isNew) {
            this.isNew = isNew;
        }

        public int getIsHot() {
            return isHot;
        }

        public void setIsHot(int isHot) {
            this.isHot = isHot;
        }

        public int getOrder() {
            return order;
        }

        public void setOrder(int order) {
            this.order = order;
        }

        public String getImageUrl() {
            return imageUrl;
        }

        public void setImageUrl(String imageUrl) {
            this.imageUrl = imageUrl;
        }

        public int getIsSuggest() {
            return isSuggest;
        }

        public void setIsSuggest(int isSuggest) {
            this.isSuggest = isSuggest;
        }

        public int getIsMatch() {
            return isMatch;
        }

        public void setIsMatch(int isMatch) {
            this.isMatch = isMatch;
        }

        public int getIsTopList() {
            return isTopList;
        }

        public void setIsTopList(int isTopList) {
            this.isTopList = isTopList;
        }

        public String getBlockId() {
            return blockId;
        }

        public void setBlockId(String blockId) {
            this.blockId = blockId;
        }

        public int getIsRecommend() {
            return isRecommend;
        }

        public void setIsRecommend(int isRecommend) {
            this.isRecommend = isRecommend;
        }

        public long getCid() {
            return cid;
        }

        public void setCid(long cid) {
            this.cid = cid;
        }

        public String getH5Url() {
            return h5Url;
        }

        public void setH5Url(String h5Url) {
            this.h5Url = h5Url;
        }

        public int getRecommendOrder() {
            return recommendOrder;
        }

        public void setRecommendOrder(int recommendOrder) {
            this.recommendOrder = recommendOrder;
        }

        public String getCampCategoryId() {
            return campCategoryId;
        }

        public void setCampCategoryId(String campCategoryId) {
            this.campCategoryId = campCategoryId;
        }

        public String getCampId() {
            return campId;
        }

        public void setCampId(String campId) {
            this.campId = campId;
        }

        public int getMenuRecommendType() {
            return menuRecommendType;
        }

        public void setMenuRecommendType(int menuRecommendType) {
            this.menuRecommendType = menuRecommendType;
        }

        public int getForceRecommendOrder() {
            return forceRecommendOrder;
        }

        public void setForceRecommendOrder(int forceRecommendOrder) {
            this.forceRecommendOrder = forceRecommendOrder;
        }

        public int getScheduleType() {
            return scheduleType;
        }

        public void setScheduleType(int scheduleType) {
            this.scheduleType = scheduleType;
        }

        public int getIsTeamData() {
            return isTeamData;
        }

        public void setIsTeamData(int isTeamData) {
            this.isTeamData = isTeamData;
        }

        public int getIsPlayerData() {
            return isPlayerData;
        }

        public void setIsPlayerData(int isPlayerData) {
            this.isPlayerData = isPlayerData;
        }

        @Override
        public String toString() {
            return "MenuItemVo{" +
                    "index=" + index +
                    ", resourceType=" + resourceType +
                    ", resourceId=" + resourceId +
                    ", level=" + level +
                    ", subItems=" + subItems +
                    ", name='" + name + '\'' +
                    ", isNew=" + isNew +
                    ", isHot=" + isHot +
                    ", order=" + order +
                    ", imageUrl='" + imageUrl + '\'' +
                    ", isSuggest=" + isSuggest +
                    ", isMatch=" + isMatch +
                    ", isTopList=" + isTopList +
                    ", blockId='" + blockId + '\'' +
                    ", isRecommend=" + isRecommend +
                    ", cid=" + cid +
                    ", h5Url='" + h5Url + '\'' +
                    ", recommendOrder=" + recommendOrder +
                    ", campCategoryId='" + campCategoryId + '\'' +
                    ", campId='" + campId + '\'' +
                    ", menuRecommendType=" + menuRecommendType +
                    ", forceRecommendOrder=" + forceRecommendOrder +
                    ", scheduleType=" + scheduleType +
                    ", isAuthority=" + isAuthority +
                    ", isTeamData=" + isTeamData +
                    ", isPlayerData=" + isPlayerData +
                    '}';
        }
    }

    public long getId() {
        return id;
    }

    public void setId(long id) {
        this.id = id;
    }

    public List<MenuItemVo> getItems() {
        return items;
    }

    public void setItems(List<MenuItemVo> items) {
        this.items = items;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    @Override
    public String toString() {
        return "MenuVo{" +
                "id=" + id +
                ",name='" + name +  "'," +
                "items=" + items +
                '}';
    }
}
