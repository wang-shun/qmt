package com.lesports.qmt.web.model;

import java.util.List;

/**
* Created by ruiyuansheng on 2015/11/20.
*/
public class SearchLiveResult {

    //频道
    private String channel;
    //赛事名称
    private String gameName;
    //大项
    private String sport;
    //图片
    private String imgUrl;
    //比赛列表
    private List<HallEpisodeVo> games;

    public String getChannel() {
        return channel;
    }

    public void setChannel(String channel) {
        this.channel = channel;
    }

    public String getGameName() {
        return gameName;
    }

    public void setGameName(String gameName) {
        this.gameName = gameName;
    }

    public String getSport() {
        return sport;
    }

    public void setSport(String sport) {
        this.sport = sport;
    }

    public String getImgUrl() {
        return imgUrl;
    }

    public void setImgUrl(String imgUrl) {
        this.imgUrl = imgUrl;
    }

    public List<HallEpisodeVo> getGames() {
        return games;
    }

    public void setGames(List<HallEpisodeVo> games) {
        this.games = games;
    }

    @Override
    public String toString() {
        return "SearchLiveResult{" +
                "channel='" + channel + '\'' +
                ", gameName='" + gameName + '\'' +
                ", sport='" + sport + '\'' +
                ", imgUrl='" + imgUrl + '\'' +
                ", games=" + games +
                '}';
    }
}
